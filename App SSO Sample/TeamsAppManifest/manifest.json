  "packageName": "com.package.name",
  "name": {
    "short": "Custom Tabs and Extensions",
    "full": ""
  },
  "developer": {
    "name": "Custom Tabs and Extensions",
    "mpnId": "",
    "websiteUrl": "https://teamscustomapp.azurewebsites.net",
    "privacyUrl": "https://teamscustomapp.azurewebsites.net/index.html#/privacy",
    "termsOfUseUrl": "https://teamscustomapp.azurewebsites.net/index.html#/termsofuse"
  },
  "description": {
    "short": "Custom tabs and extensions",
    "full": "This app is to explore the features of custom app and extensions."
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "https://teamscustomapp.azurewebsites.net/index.html#/grouptabconfig",
      "canUpdateConfiguration": true,
      "scopes": [
        "groupchat",
        "team"
      ],
      "context": [
        "channelTab",
        "privateChatTab"
      ]
    }
  ],
  "staticTabs": [
    {
      "entityId": "8882efc5-85ab-4607-8e22-2f2b102ea9e4",
      "name": "Custom Tab",
