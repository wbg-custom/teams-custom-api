      "scopes": [
        "personal"
      ],
      "context": [
        "personalTab"
      ]
    },
    {
      "entityId": "about",
      "scopes": [
        "personal"
      ]
    }
  ],
  "composeExtensions": [
    {
      "botId": "e4f30e80-248c-4421-9ff8-ec1050d877b0",
      "commands": [
        {
          "id": "searchQuery",
          "type": "query",
          "title": "Search",
          "description": "Perform a search",
          "initialRun": true,
          "fetchTask": false,
          "context": [
            "commandBox",
            "compose",
            "message"
          ],
          "parameters": [
            {
              "name": "searchQuery",
              "title": "Search",
              "description": "Your search query",
              "inputType": "text"
            }
          ]
        },
        {
          "id": "UPLOADPHOTO",
          "type": "action",
          "title": "Upload",
          "description": "Upload Photo in OneDrive",
          "initialRun": false,
          "fetchTask": true,
          "context": [
            "commandBox",
            "compose"
          ],
          "parameters": [
            {
              "name": "param",
              "title": "param",
              "description": ""
            }
          ]
        },
        {
          "id": "SIGNOUTCOMMAND",
          "type": "action",
          "title": "SignOut",
          "description": "Sign Out",
          "initialRun": false,
          "fetchTask": true,
          "context": [
            "commandBox",
            "compose"
          ],
          "parameters": [
            {
              "name": "param",
              "title": "param",
              "description": ""
            }
          ]
        }
      ],
      "canUpdateConfiguration": true,
      "messageHandlers": [
        {
          "type": "link",
          "value": {
            "domains": [
              "*.botframework.com",
              "teamscustomapp.azurewebsites.net",
              "customteamsapi.azurewebsites.net",
              "*.ngrok-free.app"
            ],
            "supportsAnonymizedPayloads": false
          }
        }
      ]
    }
  ],
  "validDomains": [
    "teamscustomapp.azurewebsites.net",
    "customteamsapi.azurewebsites.net",
    "*.botframework.com",
    "*.githubusercontent.com",
    "localhost:53000",
    "*.ngrok-free.app"
  ],
  "webApplicationInfo": {
    "id": "e4f30e80-248c-4421-9ff8-ec1050d877b0",
    "resource": "api://teamscustomapp.azurewebsites.net/e4f30e80-248c-4421-9ff8-ec1050d877b0"
  },
  "devicePermissions": [
    "geolocation",
    "media",
    "notifications"
